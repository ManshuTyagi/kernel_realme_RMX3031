&otg_iddig {
	compatible = "mediatek,usb_iddig_bi_eint";
	pinctrl-names = "id_default", "id_init", "id_output_low" ;
	pinctrl-0 = <&iddig_default>;
	pinctrl-1 = <&gpio41_mode1_iddig>;
	pinctrl-2 = <&gpio41_gpio_mode>;
	status = "okay";
	qcom,otg_switch_gpio = <41>;

};
&pio {
	iddig_default: id_default {
	};
	//#ifdef VENDOR_EDIT
	gpio41_mode1_iddig: id_init {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO41__FUNC_IDDIG>;
			slew-rate = <0>;
			bias-pull-up = <101>;
		};
	};
	gpio41_gpio_mode: id_output_low {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO41__FUNC_GPIO41>;
			slew-rate = <1>;
			output-low;
		};
	};

	//#endif /* VENDOR_EDIT */
};

//#ifdef VENDOR_EDIT
//Fei.Mo@PSW.BSP.Sensor 2018-03-24, Add for motor direction switch

&pio {
    motor_default:motor_default {
    };

    boost_sleep: boost_sleep {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO15__FUNC_GPIO15>;
            slew-rate = <1>;
            bias-pull-down; //pull down
            output-low; //digital output, no invert
            drive-strength = <3>;//0.15
        };
    };

    boost_active: boost_active {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO15__FUNC_GPIO15>;
            slew-rate = <1>;
            bias-disable; //pull down
            output-high; //digital output, no invert
            drive-strength = <3>;//0.15
        };
    };

    pwm_sleep: pwm_sleep {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO90__FUNC_GPIO90>;
            slew-rate = <1>;
            bias-pull-up; //digital output, no invert
	    output-high;
            drive-strength = <0>; //LOW strength
        };
    };

    pwm_active: pwm_active {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO90__FUNC_PWM_A>;
            slew-rate = <1>;
            bias-disable; //No Pull
	    output-low;
            drive-strength = <0>; //LOW strength
        };
    };

    sleep_sleep: sleep_sleep {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO14__FUNC_GPIO14>;
	    slew-rate = <1>;
            bias-pull-up;
	    output-high;
            drive-strength = <0>; /* LOW strength */
        };
    };

    sleep_active: sleep_active {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO14__FUNC_GPIO14>;
            slew-rate = <1>;
            bias-disable; /* No Pull */
            output-low; /* digital output, no invert */
            drive-strength = <0>; /* LOW strength */
        };
    };

    dir_sleep: dir_sleep {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO16__FUNC_GPIO16>;
	    slew-rate = <1>;
            bias-pull-up;
	    output-high;
            drive-strength = <0>; /* LOW strength */
        };
    };

    dir_active: dir_active {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO16__FUNC_GPIO16>;
	    slew-rate = <1>;
            bias-disable; /* No Pull */
            output-low; /* digital output, no invert */
            drive-strength = <0>; /* LOW strength */
        };
    };
};

&motor_pl {
	status = "okay";
	compatible = "oppo-motor";
	hall-irq-pos = <170>;
	motor-retard-pos = <430>;
	delay-cycle = <14 14 14>; //ms
	slow-delay-cycle = <28 28 28>; //ms
	full-speed-param = <63 15500>;
	up-speed-param = <5 15500>;
	down-speed-param = <2 156250>;
	nodetect-speed-param = <15 15500>;
	slow-full-speed-param = <63 31250>;
	slow-up-speed-param = <5 104166>;
	slow-down-speed-param = <2 156250>;
	slow-nodetect-speed-param = <15 104166>;
	stop-neg-point = <3 3>;
	stop-pos-point = <3 3>;
	hall-data-sign = <0>; //1 represent positive ,0 represent negative
	/*support mag noise decrease*/
	mag_noise_support;
	mag_noise_pattern = <80 400 0 662 200 100 100>;
	retard-range-end = <55>;
};

&motor_drv {
	status = "okay";
	compatible = "motor_drv-8834";
	pinctrl-names = "default","motor_sleep","motor_active";
	pinctrl-0 = <&motor_default>;
	pinctrl-1 = <&sleep_sleep &pwm_sleep &dir_sleep &boost_sleep>;
	pinctrl-2 = <&sleep_active &pwm_active &dir_active &boost_active>;
	boost-gpio = <&pio 15 0x00>;
	sleep-gpio = <&pio 14 0>;
	dir-gpio = <&pio 16 0>;
	step-gpio = <&pio 90 0>;
	ratio-a = <24>;
	ratio-b = <1840>;//18.40
	ratio-c = <20>;
	ratio-d = <32>;
};

